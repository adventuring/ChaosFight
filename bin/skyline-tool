#!/bin/bash
echo "Mock SkylineTool called with: $@"
# Extract output file from arguments
for arg in "$@"; do
  if [[ "$arg" == ":output" ]]; then
    output_next=true
  elif [[ "$output_next" == "true" ]]; then
    output_file="$arg"
    output_next=false
  fi
done
echo "Output file: $output_file"
echo "rem Mock character data" > "$output_file"
echo "data mock_bitmap" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "  %00000000" >> "$output_file"
echo "data mock_color" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
echo "  \$00" >> "$output_file"
